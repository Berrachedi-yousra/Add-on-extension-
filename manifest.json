{
    "manifest_version": 2,
    "version": "1.0",
    "name": "translate",
    "description": "",
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "lib/jquery.min.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ]
}